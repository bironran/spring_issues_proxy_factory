<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
            http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
            http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd"
       default-lazy-init="false">
    <context:annotation-config/>

    <bean id="springStartupLogger" class="com.rb.springissues.SpringStartupLogger"/>

    <bean id="directoryService" class="org.springframework.aop.framework.ProxyFactoryBean">
        <property name="target" ref="validatingDirectoryService"/>
        <property name="proxyTargetClass" value="true"/>
        <property name="interceptorNames">
            <list>
                <value>directoryServiceInterceptor</value>
            </list>
        </property>
    </bean>

    <bean id="brandSettingsDao" class="com.rb.springissues.complex_sample.BrandSettingsDao"/>
    <bean id="permission" class="org.springframework.aop.framework.ProxyFactoryBean">
        <property name="target" ref="permissionInternalBean"/>
        <property name="proxyTargetClass" value="true"/>
        <property name="interceptorNames">
            <list>
                <value>permissionTransactionInterceptor</value>
            </list>
        </property>
    </bean>

    <bean id="userManager" class="com.rb.springissues.complex_sample.UserManagerImpl">
        <property name="directoryService" ref="directoryService"/>
    </bean>
    <bean id="directoryServiceInterceptor" class="com.rb.springissues.complex_sample.DirectoryServiceInterceptor"/>
    <bean id="validatingDirectoryService" class="com.rb.springissues.complex_sample.ValidatingDirectoryService">
        <property name="brandSettingsDao" ref="brandSettingsDao"/>
    </bean>
    <bean id="permissionTransactionInterceptor" class="com.rb.springissues.complex_sample.PermissionTransactionInterceptor"/>
    <bean id="permissionInternalBean" class="com.rb.springissues.complex_sample.PermissionFS2">
        <property name="directoryService" ref="directoryService"/>
    </bean>

    <bean id="abstractPcDaoDataSourceSupplier" class="com.rb.springissues.complex_sample.AbstractPcDaoDataSourceSupplier"/>

</beans>
